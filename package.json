{
  "name": "intel-math",
  "version": "5.0.3",
  "description": "Functional Math",
  "main": "dist/math.js",
  "scripts": {
    "test:dev": "npm run postversion && npm t",
    "test": "NODE_ENV=test ./bin/test.js",
    "eslint": "eslint ./",
    "jscs": "jscs .",
    "postversion": "./bin/build.sh",
    "cover:dev": "npm run cover -- --reporter=html",
    "cover": "NODE_ENV=test ./bin/build.sh cover && nyc --reporter=text npm t"
  },
  "nyc": {
    "include": [
      "/"
    ],
    "exclude": [
      "test"
    ]
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  },
  "pre-commit": [
    "jscs",
    "eslint",
    "test:dev"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.intel.com/IML/math.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "dependencies": {
    "intel-fp": "4.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-plugin-__coverage__": "0.111111.1",
    "babel-plugin-transform-flow-strip-types": "6.8.0",
    "babel-plugin-transform-strict-mode": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "eslint": "2.9.0",
    "eslint-plugin-flow-vars": "0.4.0",
    "eslint-plugin-flowtype": "2.2.7",
    "intel-jasmine-junit-reporter": "1.0.0",
    "jasmine": "2.4.1",
    "jscs": "2.11.0",
    "nyc": "6.4.2",
    "pre-commit": "1.1.2"
  }
}
